<odoo>




        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.hr.fleet.addons</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="90"/>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='end_contract_setting']" position="after">
                    <!-- <div class="app_settings_block" groups="fleet.fleet_group_manager"> -->
                        <div class="row mt16 o_settings_container" id="feul_service">
                            <div class="col-12 col-lg-6 o_setting_box">
                                <div class="o_setting_left_pane"/>
                                <div class="o_setting_right_pane">
                                      <span class="o_form_label">Feul Service</span>
                                        <field name="fuel_service_id" class="text-center oe_inline" />
                                </div>
                            </div>
                        </div>
                    <!-- </div> -->
                </xpath>
            </field>
        </record>

